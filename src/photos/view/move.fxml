<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.view.moveController">
    <Label text="Select Destination Album" />
    <ComboBox fx:id="destinationAlbumComboBox" prefWidth="200.0" />
    <HBox spacing="10.0">
        <Button onAction="#onMoveButtonClicked" text="Move" />
        <Button onAction="#onCancelButtonClicked" text="Cancel" />
    </HBox>
</VBox>