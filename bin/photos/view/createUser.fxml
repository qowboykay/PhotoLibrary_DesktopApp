<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="132.0" prefWidth="224.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.view.createUserController">
    <Label text="Create User" />
    <HBox spacing="5.0">
        <Label text="Username:" />
        <TextField fx:id="usernameField" />
    </HBox>
    <HBox spacing="5.0">
        <Label text="Password:" />
        <PasswordField fx:id="passwordNameField" />
    </HBox>
   <HBox prefHeight="0.0" prefWidth="224.0" spacing="2.0">
      <children>
          <Button onAction="#onCreateUserButtonClicked" text="Create" />
          <Button onAction="#onCancelButtonClicked" text="Cancel" />
      </children>
   </HBox>
</VBox>
